  ops a b n r j : -> Place [ctor] .

  var M : Marking .

  rl [buy] : r r r r => j j j j j j j j .
  rl [file] : j => a .
  rl [borr] : a => b .
  rl [ret] : b => a .
  rl [lose] : b => n .
  rl [disc] : a => n .
  rl [req1] : n n => r r .
  rl [req2] : M => M r .
endm

